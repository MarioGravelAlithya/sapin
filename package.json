{
  "name": "sapin",
  "license": "MIT",
  "author": "Stephane Bourbeau",
  "repository": {
    "type": "git",
    "url": "https://github.com/bourbest/sapin"
  },
  "version": "0.3.7",
  "description": "Create object validator using a declarative paradigm",
  "main": "./lib/index.js",
  "module": "./src/index.js",
  "exports": {
    ".": "./index.js"
  },
  "type": "module",
  "scripts": {
    "lint": "eslint ./src/*.js",
    "test": "nyc mocha",
    "dev": "webpack --config ./webpack.config --mode=development",
    "prod": "webpack --config ./webpack.config --mode=production --env env=production",
    "coverage": "nyc --require babel-register report --reporter=text-lcov | coveralls",
    "build": "npm run dev && npm run prod"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/preset-env": "^7.16.8",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^4.3.4",
    "coveralls": "^3.1.1",
    "eslint": "^8.7.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "standard": "^16.0.4",
    "webpack": "^5.65.0"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "redux-form",
    "validate"
  ],
  "bugs": "https://github.com/bourbest/sapin/issues",
  "dependencies": {
    "babel-loader": "^8.2.3",
    "lodash": "^4.17.21",
    "webpack-cli": "^4.9.1"
  }
}